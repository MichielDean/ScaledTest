{
  "name": "scaledtest",
  "version": "1.0.0",
  "description": "A Next.js application with Keycloak authentication",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "npm run format && npm run lint && tsc && next build",
    "dev": "docker compose -f docker/docker-compose.yml up -d && next dev --turbopack",
    "setup:test-users": "npx tsx scripts/setup-test-users.ts",
    "update-deps": "npx npm-check-updates -u && npm install",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:fix-imports": "npm run lint:fix && npm run format",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md,yml,yaml,html}\" --log-level warn",
    "test": "npx tsx scripts/build-and-test.ts",
    "test:unit": "npx tsx scripts/build-and-test.ts --selectProjects Unit",
    "test:components": "npx tsx scripts/build-and-test.ts --selectProjects Components",
    "test:integration": "npx tsx scripts/build-and-test.ts --selectProjects Integration",
    "test:system": "npx tsx scripts/build-and-test.ts --selectProjects System",
    "send-test-results": "npx tsx scripts/send-test-results.ts",
    "demo-data": "npx tsx scripts/generate-demo-data.ts",
    "generate-ctrf:types": "npx json2ts -i src/schemas/ctrf/ctrf-schema.json -o src/schemas/ctrf/ctrf.ts --inferStringEnumKeysFromValues --enableConstEnums",
    "prepare": "husky",
    "postinstall": "husky && npx playwright install chromium",
    "kill": "kill-port 3000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.1",
    "@nivo/bar": "^0.99.0",
    "@nivo/core": "^0.99.0",
    "@nivo/line": "^0.99.0",
    "@nivo/pie": "^0.99.0",
    "@nivo/scatterplot": "^0.99.0",
    "@opensearch-project/opensearch": "^3.5.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.19.1",
    "get-port": "^7.1.0",
    "jose": "^6.0.11",
    "keycloak-js": "^26.2.0",
    "kill-port": "^2.0.1",
    "lucide-react": "^0.523.0",
    "next": "^15.3.3",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.62.0",
    "recharts": "^2.15.3",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/kill-port": "^2.0.3",
    "@types/node": "^22.15.29",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/wait-on": "^5.3.4",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "autoprefixer": "^10.4.21",
    "axe-playwright": "^2.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.0",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-ctrf-json-reporter": "^0.0.9",
    "jest-environment-jsdom": "^30.0.0",
    "jest-environment-node": "^29.7.0",
    "json-schema-to-typescript": "^15.0.4",
    "lint-staged": "^16.1.0",
    "playwright": "^1.52.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "shadcn-ui": "^0.9.5",
    "supertest": "^7.1.1",
    "tailwindcss": "^4.1.12",
    "ts-jest": "^29.3.4",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "wait-on": "^8.0.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md,yml,yaml,html}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "glob": "10.4.5",
    "nyc": {
      "glob": "^7.2.3"
    }
  }
}
