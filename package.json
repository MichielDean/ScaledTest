    "lint-staged": "^16.1.6",
    "node-pg-migrate": "^8.0.3",
    "pino-pretty": "^13.1.1",
    "playwright": "^1.55.0",
    "pm2": "^6.0.10",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "shadcn-ui": "^0.9.5",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.12",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "uuid": "^11.1.0",
    "wait-on": "^8.0.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md,yml,yaml,html}": [
