{
  "name": "scaledtest",
  "version": "1.0.0",
  "description": "A Next.js application with Better Auth authentication",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "npx tsx scripts/build.ts",
    "dev": "npm run pm2:dev",
    "start": "npm run migrate && next start",
    "migrate": "npx @better-auth/cli migrate --yes && npx node-pg-migrate up --migrations-dir migrations/scaledtest",
    "update-deps": "npx npm-check-updates -u && npm install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md,yml,yaml,html}\" --log-level warn",
    "lint": "eslint .",
    "test": "npx tsx scripts/build-and-test.ts",
    "test:unit": "npx tsx scripts/build-and-test.ts --selectProjects Unit",
    "test:components": "npx tsx scripts/build-and-test.ts --selectProjects Components",
    "test:integration": "npx tsx scripts/build-and-test.ts --selectProjects Integration",
    "test:system": "npx tsx scripts/build-and-test.ts --selectProjects System",
    "send-test-results": "npx tsx scripts/send-test-results.ts",
    "generate-ctrf:types": "npx json2ts -i src/schemas/ctrf/ctrf-schema.json -o src/schemas/ctrf/ctrf.ts --inferStringEnumKeysFromValues --enableConstEnums",
    "prepare": "husky",
    "postinstall": "husky && npx playwright install chromium",
    "kill": "kill-port 3000",
    "pm2:dev": "npx tsx scripts/manage-pm2-server.ts start",
    "pm2:start": "npx tsx scripts/manage-pm2-server.ts start",
    "pm2:stop": "npx tsx scripts/manage-pm2-server.ts stop",
    "pm2:clean": "npx tsx scripts/manage-pm2-server.ts clean",
    "pm2:delete": "npx tsx scripts/manage-pm2-server.ts delete",
    "pm2:status": "npx pm2 status",
    "pm2:update": "npx pm2 update",
    "pm2:logs": "npx tsx scripts/manage-pm2-server.ts logs",
    "pm2:restart": "npx tsx scripts/manage-pm2-server.ts restart"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.1",
    "@nivo/bar": "^0.99.0",
    "@nivo/core": "^0.99.0",
    "@nivo/line": "^0.99.0",
    "@nivo/pie": "^0.99.0",
    "@nivo/scatterplot": "^0.99.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "axios": "^1.12.1",
    "better-auth": "^1.3.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "get-port": "^7.1.0",
    "kill-port": "^2.0.1",
    "lucide-react": "^0.542.0",
    "next": "^15.5.3",
    "pg": "^8.16.3",
    "pino": "^9.9.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
